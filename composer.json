{
  "name": "computablefacts/cywise",
  "description": "Cywise",
  "keywords": [
    "laravel",
    "cywise",
    "cybersecurity"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.3",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-pcntl": "*",
    "24slides/laravel-saml2": "^2.4",
    "azure-oss/storage-blob-laravel": "^1.3",
    "botman/botman": "^2.8",
    "botman/driver-web": "^1.5",
    "erusev/parsedown": "^1.7",
    "guzzlehttp/guzzle": "^7.5",
    "laravel/cashier": "^15.4",
    "laravel/framework": "^11.44",
    "laravel/sanctum": "^4.0",
    "laravel/telescope": "^5.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^4.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/flysystem-path-prefixing": "^3.0",
    "league/html-to-markdown": "^5.1",
    "phpseclib/phpseclib": "^3.0",
    "sajya/server": "^7.0",
    "spatie/laravel-health": "^1.34",
    "spatie/laravel-package-tools": "^1.18",
    "symfony/process": "^7.0",
    "symfony/yaml": "^7.2",
    "teamtnt/laravel-scout-tntsearch-driver": "^15.0",
    "vanilo/admin": "^4.0",
    "vanilo/framework": "^4.0",
    "vanilo/payment": "^4.0",
    "webklex/laravel-imap": "^6.1",
    "ext-pdo": "*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.13",
    "fakerphp/faker": "^1.23",
    "laravel-lang/lang": "^15.7",
    "laravel/dusk": "^8.2",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^11.2",
    "spatie/laravel-ignition": "^2.5"
  },
  "autoload": {
    "files": [
      "app/Helpers/Helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan lang:update"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
